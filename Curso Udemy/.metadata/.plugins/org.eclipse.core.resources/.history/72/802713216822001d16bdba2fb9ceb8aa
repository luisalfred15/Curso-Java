public class App6 {
	public static void main(String[] args) {
		
		long poblacion = 30;
		int contagios = 1;
		
		for (long k = poblacion; k > 0; k--) {
			contagios = 2 * contagios;
			
			if (contagios <= 5) {
				System.out.println("La pandemia esta empezando");
			} else if (contagios <= 10) {
				System.out.println("Ya se contagio un tercio de la poblacion");
			} else if (contagios <= 20) {
				System.out.println("Ya casi todos contagiados :(");
			} else {
				System.out.println("Todos estan contagiados :(");
			}
		}
	
	}
}

// Sintx: if (cond. a ser V) {
//		(bloque de codigo)
// } else if {
//
// } else if {
//
// } 
// ...
// } else {
//
// {